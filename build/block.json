{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rm/swiper-container",
  "version": "0.1.0",
  "title": "Swiper Container",
  "category": "widgets",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "alignWide": true,
    "layout": {
      "allowCustomContentAndWideSize": true,
      "default": {
        "type": "flex",
        "orientation": "horizontal",
        "flexWrap": "nowrap"
      }
    },
    "color": {
      "gradients": true,
      "background": true
    },
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "shadow": true,
    "anchor": true,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "width": true,
      "style": true
    }
  },
  "attributes": {
    "align": {
      "type": "string"
    },
    "slidesPerView": {
      "type": "string",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "slides-per-view"
    },
    "spaceBetween": {
      "type": "string",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "space-between",
      "default": "0px"
    },
    "autoPlay": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "autoplay"
    },
    "navigation": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "navigation"
    },
    "pagination": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "pagination"
    },
    "scrollbar": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "scrollbar"
    },
    "loop": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "loop"
    },
    "shouldOverflow": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "should-overflow"
    },
    "speed": {
      "type": "string",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "speed",
      "default": "300"
    },
    "centeredSlides": {
      "type": "boolean",
      "source": "attribute",
      "selector": "swiper-container",
      "attribute": "centered-slides"
    }
  },
  "textdomain": "swiper-container",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}